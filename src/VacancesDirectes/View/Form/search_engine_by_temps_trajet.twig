{% extends "base.twig" %}

{% block javascripts %}
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
    <script src="http://www.mapnificent.net/media/api/1/mapnificent.js" type="text/javascript" charset="utf-8"></script>
    <script type="text/javascript" src="{{ asset('/vacancesdirectes/js/search/maps.js') }}"></script>
    <script type="text/javascript">
        $(document).ready(function() {
            initialize({{ etablissements | raw }}, {{ currentLatLng[0] }}, {{ currentLatLng[1] }}, {{ temps }});
        });
    </script>
{% endblock %}

{% block content %}

        {{ message }}
        <div style="position: relative">
            <div id="map_canvas" style="width:100%; height:700px"></div>
        </div>

{% endblock %}