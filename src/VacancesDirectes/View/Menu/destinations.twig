{{ form_widget (searchForm) }}
<ul>
    <li>
        <span>Libell√©</span>
        <ul>
            {% for letter, etabs in etabByAlphabeticalOrder %}
            <li>
                <strong>{{ letter }}</strong>
                <ul>
                    {% for etab in etabs %}
                    <li><a class="camping" href="{{ etab.Id }}">{{ etab.Name }}</a></li>
                    {% endfor %}
                </ul>
            </li>
            {% endfor %}
        </ul>
    </li>
    <li>
        <span>Localisation</span>
        <ul>
            {% for region, villes in etabByVilleOrder %}
            <li>
                <a class="region">{{ region }}</a>
                <ul>
                    {% for ville, etabs in villes %}
                    <li>
                        <a class="ville">{{ ville }}</a>
                        <ul>
                            {% for etab in etabs %}
                            <li><a href="{{ etab.Id }}">{{ etab.Name }}</a></li>
                            {% endfor %}
                        </ul>
                    </li>
                    {% endfor %}
                </ul>
            </li>
            {% endfor %}
        </ul>
    </li>
</ul>