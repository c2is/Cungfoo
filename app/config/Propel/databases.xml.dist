<?xml version="1.0" encoding="UTF-8"?>
<config>
    <propel>
        <datasources default="cungfoo">
            <datasource id="cungfoo">
                <adapter>mysql</adapter>
                <connection>
                    <dsn>mysql:host=##host##;charset=utf8;dbname=##database##</dsn>
                    <user>##username##</user>
                    <password>##password##</password>
                </connection>
            </datasource>
        </datasources>
    </propel>
</config>
